    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint --fix 'src/**/*.ts'",
    "test": "ng test --no-watch",
    "test:watch": "ng test --watch",
    "test:ci": "yarn test",
    "build:storybook": "build-storybook -c .storybook -s .storybook/public -o dist/docs/storybook",
    "ng": "ng",
    "ng:build": "ngcc && ngcc -s ../../node_modules && ng build",
    "clean": "rm -rf dist",
    "start": "echo 'script not defined' && exit 0",
    "format": "echo 'script not defined' && exit 0",
    "format:check": "echo 'script not defined' && exit 0",
    "postinstall": "node scripts/vendor.js",
    "prepublishOnly": "node scripts/sync-version.js"
  },
  "repository": {
    "type": "git",
